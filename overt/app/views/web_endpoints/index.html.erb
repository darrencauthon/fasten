<div class="row">

  <div class="col-md-12">
    <div class="box">
      <div class="box-header with-border">
        <h3 class="box-title">Web Endpoints</h3>
      </div>
      <div class="box-body">
        <table class="table table-bordered">
	<tbody>
	  <tr>
	    <th>Name</th>
	  </tr>
          <% @steps.each do |step| %>
	    <tr>
	      <td><a href="/web_endpoints/view/<%= step[:workflow_id] %>/<%= step[:id] %>"><%= step[:name] %></a></td>
	    </tr>
	  <% end %>
	</tbody>
	</table>
      </div>
      <div class="box-footer">
      </div>
    </div>
  </div>

</div>

