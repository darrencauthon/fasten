      ]
    },
    {
      "id": "pull_out_links",
      "name": "Pull out all links",
      "label": "Pull out all links",
      "type": "HtmlParser",
      "config": {
        "extract": {
          "url": {
            "css": "a",
            "value": "@href"
          },
          "text": {
            "css": "a",
            "value": "./node()"
          }
        },
        "path": "body"
      },
      "parent_step_ids": [
        "get_url"
      ]
    }
  ],
  "id": "ping"
