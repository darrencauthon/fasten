{
  "context": {
  },
  "steps": [
    {
      "id": "look_up_data",
      "name": "Start Ping of URL",
      "label": "Start Ping of URL",
      "type": "ManualStart",
      "message": "Starting {{url}}",
      "config": {
      },
      "parent_step_ids": [

      ]
    },
    {
      "id": "get_url",
      "name": "Call out to URL",
      "label": "Call out to URL",
      "type": "WebRequest",
      "config": {
        "url": "{{url}}"
      },
      "message": "Hitting {{url}}",
      "parent_step_ids": [
        "look_up_data"
      ]
    },
    {
      "id": "only-200s",
      "name": "Only 200s",
      "label": "Only 200s",
      "type": "Trigger",
      "parent_step_ids": [
        "get_url"
      ],
      "config": {
        "rules": [
          {
            "path": "status",
            "type": "==",
            "value": "200"
          }
        ]
      },
      "message": "{{url}} is good"
    },
    {
      "id": "test-format",
      "name": "Test the formatting",
      "label": "Test the formatting",
      "type": "EventFormatter",
      "parent_step_ids": [
        "only-200s"
      ],
      "config": {
        "instructions": {
          "body": "{{body | replace: \"\r\n\", \"\"}}"
        }
      }
    },
    {
      "id": "get-the-links",
      "name": "Get The Links",
      "label": "Get The Links",
      "type": "HtmlParser",
      "parent_step_ids": [
        "test-format"
      ],
      "config": {
        "extract": {
          "url": {
            "css": "a",
            "value": "@href"
          }
        },
        "path": "body"
      }
    }
  ],
  "id": "ping"
}