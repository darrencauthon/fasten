        "look_up_data"
      ]
    },
    {
      "id": "only-200s",
      "name": "Only 200s",
      "label": "Only 200s",
      "type": "Trigger",
      "parent_step_ids": [
        "get_url"
      ],
      "config": {
        "rules": [
          {
            "path": "status",
            "type": "==",
            "value": "200"
          }
        ]
      },
      "message": "{{url}} is good"
    }
  ],
  "id": "ping"
}